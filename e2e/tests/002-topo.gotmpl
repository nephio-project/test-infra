name: free5gc-net
prefix: net
topology:
  kinds:
    srl:
      image: ghcr.io/nokia/srlinux
  nodes:
    N6:
      kind: srl
{{- range .workers }}
    {{ . }}:
      kind: ext-container
{{- end }}
  links:
{{- range $i, $worker := .workers }}
    - endpoints: ["N6:e1-{{ $i }}", "{{ $worker }}:eth1"]
{{- end }}
